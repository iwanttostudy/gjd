page{background-color:#f5f5f5}.goods-info{display:-webkit-box;display:-webkit-flex;display:flex;background-color:#fff;margin-bottom:20rpx;padding:20rpx}.goods-info .info-right{margin-left:20rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column}.goods-info .info-right .price{color:red;margin-top:auto}.goods-info .info-right .price .num{color:#999}.cell{background-color:#fff;padding:20rpx;margin-bottom:20rpx}.cell .cell-item{display:-webkit-box;display:-webkit-flex;display:flex;padding:20rpx 0;border-bottom:1px solid #f5f5f5}.cell .cell-item text:nth-child(1){white-space:nowrap;color:#616161}.cell .cell-item text:nth-child(2){margin-left:auto}.cell .cell-item:last-child{border-bottom:0}.footer-btn-box{margin-top:20rpx}